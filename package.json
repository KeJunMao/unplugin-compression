{
  "name": "unplugin-compression",
  "version": "0.0.3",
  "homepage": "https://github.com/KeJunMao/unplugin-compression",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeJunMao/unplugin-compression.git"
  },
  "description": "Compress dist to `zip`, `tar`, `taz`.",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "esno src/index.ts",
    "test": "vitest",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish"
  },
  "keywords": [
    "unplugin",
    "compression",
    "dist"
  ],
  "author": "KeJun <kejun1997@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.16.3",
    "bumpp": "^9.1.0",
    "esno": "^0.16.3",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.4",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "compressing": "^1.9.0",
    "picocolors": "^1.0.0",
    "unplugin": "^1.3.1"
  }
}
